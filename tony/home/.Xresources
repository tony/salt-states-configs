URxvt.foreground:       white
URxvt.background:       black
URxvt.font:             xft:Terminus,xft:WenQuanYi Zen Hei Mono,xft:unifont,xft:Symbola
URxvt.boldFont:         xft:Terminus,xft:WenQuanYi Zen Hei Mono,xft:unifont,xft:Symbola
!! --- URxvt ---
!!URxvt.font:xft:Source Code Pro-10:xft:Terminus-11:hint=True:antialias=False,xft:WenQuanYi Micro Hei:pixelsize=14:antialias=False
!!URxvt.boldFont:xft:Source Code Pro-10:xft:Terminus-11:bold:hint=True:antialias:False,xft:WenQuanYi Micro Hei:pixelsize=11:antialias=True,xft:Code2000:pixelsize=12:antialias=False
URxvt*intensityStyles: false
URxvt*internalBorder: 1
URxvt.keysym.C-plus: command:\033]710;xft:monospace:size=9\007\033]711;xft:monospace:size=9\007
URxvt.keysym.C-minus: command:\033]710;xft:monospace:size=8\007\033]711;xft:monospace:size=8\007
#URxvt*transparent: true
#URxvt*.shading:  25

URxvt.inputMethod: ibus
URxvt.preeditType: OnTheSpot,None

URxvt*scrollBar_right: false
URxvt*scrollBar: false

URxvt.perl-ext-common: default,matcher,tabbed
URxvt.tabbed.tabbar-bg: -1
URxvt.scrollBar:        false
URxvt.urgentOnBell:     true
URxvt.imLocale:         en_GB.UTF-8
URxvt.cursorBlink:      true
URxvt.cursorColor:      #ff9900
URxvt.cursorColor2:     #000000
URxvt.colorBD:          #ffffff
URxvt.colorIT:          #bbbbbb
URxvt.colorUL:          #ff9900
URxvt.underlineColor:   #999999
URxvt.borderless:       true
URxvt*perl-ext-common:  default,matcher
URxvt*urlLauncher:      x-www-browser
URxvt*matcher.button:   1
URxvt.searchable-scrollback: CM-s

